{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Routes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "google.com",
          "Port": 443,
          "scheme": "Https"
        },
        {
          "Host": "vk.com",
          "Port": 443,
          "scheme": "Https"
        }
      ],
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": [ "Get", "post" ],
      "LoadBalancerOptions": {
        "Type": "CustomLoadBalancer"
      }
    }
  ]
}
